{
  "name": "multi-player",
  "version": "0.0.0",
  "private": true,
  "config": {
    "dockertag": "relang.azurecr.io/mpgame/server"
  },
  "scripts": {
    "docker:build": "docker build -t $npm_package_config_dockertag .",
    "docker:run": "docker run --rm -p 8080:8080 $npm_package_config_dockertag",
    "docker:push": "docker push $npm_package_config_tag",
    "start": "node ./bin/www",
    "start:w": "nodemon",
    "test:w": "jest --watch",
    "build": "tsc"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "^1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "jest": "^25.5.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
